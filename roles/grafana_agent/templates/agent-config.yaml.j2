server:
  log_level: info
  http_listen_port: 12345

logs:
  configs:
    - name: default
      clients:
        - url: "{{ loki_address }}"
      positions:
        filename: /var/lib/grafana-agent-positions.yaml
      scrape_configs:
        - job_name: system-logs
          static_configs:
            - targets: ['localhost']
              labels:
                job: varlogs
          __path__: {{ agent_log_paths }}
